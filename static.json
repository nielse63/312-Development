{
  "root": "dist/",
  "https_only": true,
  "clean_urls": true,
  "error_page": "404.html",
  "routes": {
    "/**": "index.html"
  },
  "proxies": {
    "/api/": {
      "origin": "${API_HOST}/api"
    }
  },
  "headers": {
    "/": {
      "Cache-Control": "no-store, no-cache"
    },
    "**.js": {
      "Cache-Control": "public, max-age=86400"
    },
    "**.css": {
      "Cache-Control": "public, max-age=86400"
    },
    "**.jpg": {
      "Cache-Control": "public, max-age=86400"
    }
  }
}
