    "yorkie": "^2.0.0"
  },
  "optionalDependencies": {
    "lerna": "^3.0.0-rc.0"
  },
  "engines": {
    "node": ">= 10.x",
    "npm": ">= 6.x",
    "yarn": ">= 1.x"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
