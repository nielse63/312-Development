{
  "name": "312-development",
  "version": "2.0.0",
  "description": "The portfolio site of Erik Nielsen, UI/UX engineer from Chicago.",
  "author": "Erik Nielsen <erik@312development.com>",
  "homepage": "https://312development.com/",
  "private": true,
  "scripts": {
    "prebuild": "rm -rf dist/",
    "build": "cross-env NODE_ENV=production webpack --mode production --colors",
    "postbuild": "bin/post-build",
    "serve": "cross-env NODE_ENV=development webpack-dev-server --hot",
    "start": "bin/start",
    "lint:js": "eslint --ext .js,.vue ./*.js src/ build/ test/ --fix",
    "lint:scss": "bin/lint scss",
    "lint:md": "markdownlint --config .mdlrc docs/ README.md",
    "seo": "seolint https://312development.com/",
    "duplicate": "jsinspect src/",
    "new": "vg",
    "test:e2e": "bin/test e2e",
    "test:unit": "jest --config=test/unit/jest.config.js",
    "test": "bin/test --all",
    "heroku-postbuild": "yarn build"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "dependencies": {
    "offline-plugin": "^5.0.5",
    "vue": "^2.5.17",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@vue/test-utils": "^1.0.0-beta.24",
    "autoprefixer": "^9.1.3",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.6",
    "babel-jest": "^23.4.2",
    "babel-loader": "^7.1.5",
    "babel-plugin-dynamic-import-node": "^2.0.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-2": "^6.24.1",
    "compression-webpack-plugin": "^1.1.11",
    "copy-webpack-plugin": "^4.5.2",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.0",
    "dotenv": "^6.0.0",
    "eslint": "^5.4.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-import-resolver-webpack": "^0.10.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-unicorn": "^6.0.0",
    "eslint-plugin-vue": "^4.7.1",
    "file-loader": "^2.0.0",
    "html-critical-webpack-plugin": "^2.1.0",
    "html-webpack-plugin": "^3.2.0",
    "img-loader": "^3.0.0",
    "jest": "^23.5.0",
    "jest-canvas-mock": "^1.1.0",
    "jest-puppeteer": "^3.3.1",
    "jsinspect": "^0.12.7",
    "lint-staged": "^7.2.2",
    "markdownlint-cli": "^0.13.0",
    "mini-css-extract-plugin": "^0.4.2",
    "node-sass": "^4.9.3",
    "optimize-css-assets-webpack-plugin": "^5.0.0",
    "postcss-html": "^0.33.0",
    "postcss-loader": "^3.0.0",
    "postcss-scss": "^2.0.0",
    "postcss-syntax": "^0.33.0",
    "puppeteer": "^1.7.0",
    "resource-hints-webpack-plugin": "^0.0.2",
    "sass-loader": "^7.1.0",
    "seolint": "^0.14.2",
    "style-loader": "^0.23.0",
    "stylelint": "^9.5.0",
    "stylelint-config-recommended-scss": "^3.2.0",
    "stylelint-scss": "^3.3.0",
    "svg-inline-loader": "^0.8.0",
    "uglifyjs-webpack-plugin": "^1.3.0",
    "vue-generator": "^1.1.0",
    "vue-jest": "^2.6.0",
    "vue-loader": "^15.4.1",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.5.17",
    "webpack": "^4.17.1",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.6",
    "webpack-merge": "^4.1.4",
    "webpack-pwa-manifest": "^3.7.1",
    "yorkie": "^2.0.0"
  },
  "engines": {
    "node": ">= 10.x",
    "npm": ">= 6.x",
    "yarn": ">= 1.x"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "config": {
    "commitizen": {
      "path": "cz-emoji"
    }
  }
}
