  "scripts": {
    "clean": "rm -rf ./build",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --inline --hot --progress --colors",
    "start": "superstatic build -p 5001 --host localhost --gzip -c .superstatic.json",
    "prestart": "npm run build",
    "build": "cross-env NODE_ENV=production webpack --cache false -p --progress",
    "prebuild": "npm run clean && mkdirp build && ncp src/assets build/assets",
    "test": "npm run -s lint",
    "test:karma": "karma start test/karma.conf.js --single-run",
    "lint": "./node_modules/.bin/eslint ./src --fix",
    "resize-images": "node ./tasks/image-resize.js",
    "surge": "npm run lint && npm run build && surge ./build -d https://312development.surge.sh"
  },
  "author": "Erik Nielsen",
