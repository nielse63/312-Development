{
  "name": "312-development",
  "version": "1.0.50",
  "description": "My personal website.",
  "bugs": {
    "url": "https://github.com/nielse63/312-Development/issues",
    "email": "erik@312development.com"
  },
  "license": "MIT",
  "repository": "312-Development.git",
  "main": "app-server.js",
  "engine": {
    "node": ">=7.2.1",
    "npm": ">=4.0.5"
  },
  "dependencies": {
    "connect": "^3.5.0",
    "cross-env": "^3.1.3",
    "express": "^4.14.0",
    "fg-loadcss": "^1.2.0",
    "heroku-ssl-redirect": "0.0.3",
    "jquery": "^3.1.1",
    "moment": "^2.17.1",
    "offline-plugin": "^4.5.3",
    "preact": "^7.1.0",
    "preact-router": "^2.3.2",
    "string": "^3.3.3"
  },
  "scripts": {
    "clean": "rm -rf ./build",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --inline --hot --progress --colors",
    "start": "NODE_ENV=production node --optimize_for_size --max_old_space_size=920 --gc_interval=100 app.js",
    "prestart": "npm run build",
    "build": "cross-env NODE_ENV=production webpack -p --progress",
    "prebuild": "npm run clean && mkdir build && cp -r src/assets build/assets",
    "test": "npm run lint",
    "lint": "npm run lint:js && npm run lint:scss",
    "lint:js": "./node_modules/.bin/eslint ./src --fix",
    "lint:scss": "./node_modules/.bin/sass-lint 'src/**/*.scss' -v",
    "resize-images": "node ./tasks/image-resize.js",
    "surge": "npm run lint && npm run build && surge ./build -d https://312development.surge.sh"
  },
  "author": "Erik Nielsen",
  "devDependencies": {
    "autoprefixer": "^6.4.0",
    "babel": "^6.5.2",
    "babel-core": "^6.14.0",
    "babel-eslint": "^7.0.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-react-jsx": "^6.8.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-es2015-minimal": "^2.0.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.14.0",
    "babel-runtime": "^6.11.6",
    "copy-webpack-plugin": "^4.0.1",
    "core-js": "^2.4.1",
    "css-loader": "^0.25.0",
    "eslint": "^3.12.1",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.8.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.22.0",
    "json-loader": "^0.5.4",
    "nightmare": "^2.8.1",
    "node-sass": "^3.13.1",
    "path": "^0.12.7",
    "postcss-loader": "^0.13.0",
    "raw-loader": "^0.5.1",
    "sass-lint": "^1.10.2",
    "sass-loader": "^4.0.2",
    "source-map-loader": "^0.1.5",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.14.0",
    "webpack-dev-server": "^1.15.0"
  },
  "sasslintConfig": "./.scsslint.yml"
}
