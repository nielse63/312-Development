{
  "author": "Erik Nielsen",
  "name": "312-development",
  "version": "1.0.50",
  "description": "My personal website.",
  "homepage": "https://312development.com",
  "bugs": {
    "url": "https://github.com/nielse63/312-Development/issues",
    "email": "erik@312development.com"
  },
  "license": "MIT",
  "repository": "https://github.com/nielse63/312-Development.git",
  "main": "index.js",
  "engines": {
    "node": "7.8.0",
    "npm": "4.5.0"
  },
  "scripts": {
    "dev": "NODE_ENV=dev-server webpack-dev-server --inline --hot --progress --colors",
    "lint:js": "eslint --fix src tasks test ./*.js",
    "lint:scss": "scss-lint src/",
    "lint": "yarn run lint:js",
    "clean": "rm -rf ./build",
    "tasks": "node ./tasks",
    "prebuild": "yarn run clean && yarn run lint",
    "build": "NODE_ENV=production webpack -p --progress",
    "postbuild": "yarn run tasks",
    "prestart": "yarn run build",
    "start": "yarn run local:server",
    "local:server": "node --optimize_for_size --max_old_space_size=920 --gc_interval=100 index.js",
    "pretest": "yarn run lint",
    "test:local": "mocha ./test/index.js",
    "test": "echo 'No tests configure' && exit",
    "profile": "webpack --profile -p --display-error-details --display-reasons --display-modules --json > webpack.profile.json",
    "karma": "NODE_ENV=test karma start test/karma.conf.js --single-run"
  },
  "dependencies": {
    "axios": "^0.16.0",
    "babel-polyfill": "^6.23.0",
    "body-parser": "^1.17.1",
    "compression": "^1.6.2",
    "express": "^4.15.2",
    "express-minify-html": "^0.10.0",
    "fg-loadcss": "^1.3.1",
    "heroku-ssl-redirect": "0.0.3",
    "hogan-express": "^0.5.2",
    "preact": "^7.2.1",
    "preact-router": "^2.4.1",
    "proptypes": "^0.14.4",
    "serviceworker-webpack-plugin": "^0.2.1",
    "store": "^2.0.4"
  },
  "devDependencies": {
    "autoprefixer": "^6.7.7",
    "babel-core": "^6.24.0",
    "babel-eslint": "^7.2.1",
    "babel-loader": "^6.4.1",
    "babel-plugin-transform-react-jsx": "^6.23.0",
    "babel-preset-env": "^1.3.2",
    "babel-preset-react": "^6.23.0",
    "babel-register": "^6.24.0",
    "chai": "^3.5.0",
    "color-namer": "^1.1.0",
    "compression-webpack-plugin": "^0.3.2",
    "copy-webpack-plugin": "^4.0.1",
    "critical": "^0.8.4",
    "css": "^2.2.1",
    "css-loader": "^0.28.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "extract-text-webpack-plugin": "^2.1.0",
    "favicons-webpack-plugin": "^0.0.7",
    "file-loader": "^0.11.1",
    "fontmin": "^0.9.6",
    "html-webpack-plugin": "^2.28.0",
    "imagemin": "^5.2.2",
    "imagemin-mozjpeg": "^6.0.0",
    "imagemin-pngquant": "^5.0.0",
    "imagemin-svgo": "^5.2.1",
    "json-loader": "^0.5.4",
    "mocha": "^3.2.0",
    "node-sass": "^4.5.2",
    "postcss-loader": "^1.3.3",
    "preact-compat": "^3.14.3",
    "raw-loader": "^0.5.1",
    "sass-loader": "^6.0.3",
    "shelljs": "^0.7.7",
    "style-loader": "^0.16.1",
    "url-loader": "^0.5.8",
    "webpack": "^2.3.3",
    "webpack-dev-server": "^2.4.2"
  },
  "cacheDirectories": [
    "node_modules",
    "$HOME/.cache/yarn",
    "$HOME/.yarn"
  ]
}
