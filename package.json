{
  "name": "312-development",
  "version": "2.0.0",
  "description": "The portfolio site of Erik Nielsen, UI/UX engineer from Chicago",
  "author": "Erik Nielsen <erik@312development.com>",
  "private": true,
  "scripts": {
    "prebuild": "rm -rf dist/",
    "build": "cross-env NODE_ENV=production webpack --mode production --colors",
    "serve": "cross-env NODE_ENV=development webpack-dev-server --hot",
    "start": "bin/start",
    "lint:js": "eslint --ext .js,.vue ./*.js src/ build/ test/ --fix",
    "lint:scss": "bin/lint scss",
    "lint:md": "remark -S **/*.md -o",
    "new": "vg",
    "test:e2e": "bin/test e2e",
    "test:unit": "jest --config=test/unit/jest.config.js",
    "heroku-postbuild": "yarn build",
    "commit": "git add . && git cz"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "dependencies": {
    "offline-plugin": "^5.0.5",
    "vue": "^2.5.17",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@vue/test-utils": "^1.0.0-beta.23",
    "autoprefixer": "^9.0.2",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.6",
    "babel-jest": "^23.4.2",
    "babel-loader": "^7.1.5",
    "babel-plugin-dynamic-import-node": "^2.0.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-2": "^6.24.1",
    "body-parser": "^1.18.3",
    "case": "^1.5.5",
    "compression-webpack-plugin": "^1.1.11",
    "copy-webpack-plugin": "^4.5.2",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.0",
    "dotenv": "^6.0.0",
    "ejs": "^2.6.1",
    "eslint": "^5.2.0",
    "eslint-config-airbnb-base": "^13.0.0",
    "eslint-import-resolver-webpack": "^0.10.1",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-unicorn": "^5.0.0",
    "eslint-plugin-vue": "^4.7.1",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "faker": "^4.1.0",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "image-size": "^0.6.3",
    "img-loader": "^3.0.0",
    "inject-loader": "^4.0.1",
    "jest": "^23.4.2",
    "jest-puppeteer": "^3.2.1",
    "lint-staged": "^7.2.0",
    "mini-css-extract-plugin": "^0.4.1",
    "node-sass": "^4.9.2",
    "optimize-css-assets-webpack-plugin": "^5.0.0",
    "postcss-html": "^0.32.0",
    "postcss-loader": "^3.0.0",
    "postcss-scss": "^2.0.0",
    "postcss-syntax": "^0.33.0",
    "puppeteer": "^1.6.1",
    "remark-cli": "^5.0.0",
    "remark-preset-lint-markdown-style-guide": "^2.1.2",
    "resource-hints-webpack-plugin": "^0.0.2",
    "sass-loader": "^7.1.0",
    "sharp": "^0.20.5",
    "style-loader": "^0.22.0",
    "stylelint": "^9.4.0",
    "stylelint-config-recommended-scss": "^3.2.0",
    "stylelint-scss": "^3.2.0",
    "svg-inline-loader": "^0.8.0",
    "svgo": "^1.0.5",
    "uglifyjs-webpack-plugin": "^1.2.7",
    "vue-generator": "^1.1.0",
